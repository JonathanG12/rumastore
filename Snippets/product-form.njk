<form @submit.prevent="onSubmit">
  <div class="bg-card/75 border-b border-x border-white/5 text-t-primary p-4 md:p-6">
    {% if product.type == 'variant' %}
    <div class="mb-4">
      <label for="variant" class="block mb-2 text-sm">Variant</label>
      {% if properties.variant_selector == 'dropdown' or is_embed %}
      <select name="variant" @change="() => activeVariant = $event.target.value" class="bg-white/[0.025] text-t-primary text-sm py-2.5 px-4 w-full">
        {% for variant in product.variants %}
          <option value="{{ loop.index0 }}" :selected="activeVariant == {{ loop.index0 }}">
            {{ variant.name }} ({{ formatPrice(variant.price, product.currency) }})
          </option>
        </template>
        {% endfor %}
      </select>
      {% else %}
      <div class="flex flex-col gap-2">
        {% for variant in product.variants %}
        <button
          type="button"
          class="flex items-center justify-between px-4 py-2 transition-colors duration-300 ease-in-out border text-t-primary bg-none border-transparent"
          :class="`${activeVariant == {{ loop.index0 }} ? 'bg-white/[0.025] border-white/5' : 'bg-none border-transparent'} ${({{ 'true' if variant.stock === 0 else 'false' }} || activeVariant == {{ loop.index0 }}) ? '' : 'hover:bg-white/[0.025]'}`"
          @click="() => activeVariant = {{ loop.index0 }}"
          {{ 'title="Out of Stock"' if variant.stock === 0 }}
          {{ 'disabled' if variant.stock === 0 }}
        >
          <div class="flex flex-col items-start gap-1 text-left">
            <p class="text-base">{{ variant.name }}</p>
            <p class="text-xs">
              {% if variant.stock === 0 %}
              <span>Out of stock</span>
              {% elif product.hide_stock_count %}
              <span>In stock</span>
              {% elif variant.stock === -1 %}
              <span class="inline-flex items-center gap-1">
                <svg xmlns="http://www.w3.org/2000/svg" class="text-accent-500 size-4" fill="currentColor" viewBox="0 0 256 256"><path d="M248,128a56,56,0,0,1-95.6,39.6l-.33-.35L92.12,99.55a40,40,0,1,0,0,56.9l8.52-9.62a8,8,0,1,1,12,10.61l-8.69,9.81-.33.35a56,56,0,1,1,0-79.2l.33.35,59.95,67.7a40,40,0,1,0,0-56.9l-8.52,9.62a8,8,0,1,1-12-10.61l8.69-9.81.33-.35A56,56,0,0,1,248,128Z"></path></svg>
                in stock
              </span>
              {% else %}
              <span>{{ variant.stock }} in stock</span>
              {% endif %}
            </p>
          </div>
          <div class="bg-white/[0.025] text-t-primary text-sm px-2 py-1">
            <span>{{ formatPrice(variant.price, product.currency) }}</span>
            {% if variant.price_slash %}
              <span class="text-xs text-t-primary/50 line-through">{{ formatPrice(variant.price_slash, product.currency) }}</span>
            {% endif %}
          </div>
        </button>
        {% endfor %}
      </div>
      {% endif %}
      <small x-show="!(activeVariant in product.variants)" x-cloak class="text-red-500">
        Please select a variant.
      </small>
    </div>
    {% endif %}
    <div class="mb-4">
      <label for="quantity" class="block mb-2 text-sm">Quantity</label>
      <div class="flex overflow-hidden focus-within:border-accent-500 focus-within:ring-accent-500 focus-within:ring-2 focus-within:ring-opacity-50">
        <button
          @click="quantityDecrease"
          type="button"
          class="bg-white/5 hover:bg-accent-500 size-[36px] flex justify-center items-center transition-colors duration-300 ease-in-out disabled:pointer-events-none disabled:opacity-50"
          :disabled="outOfStock"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="1.5"
            stroke="currentColor"
            class="size-5"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              d="M19.5 12h-15"
            />
          </svg>
        </button>
        <input
          type="number"
          class="w-[calc(100%-72px)] bg-white/[0.025] text-t-primary text-sm focus:outline-none py-2 px-4 disabled:opacity-50"
          name="quantity"
          x-model.number="quantity"
          :disabled="outOfStock"
        />
        <button
          @click="quantityIncrease"
          type="button"
          class="bg-white/5 hover:bg-accent-500 size-[36px] flex justify-center items-center transition-colors duration-300 ease-in-out disabled:pointer-events-none disabled:opacity-50"
          :disabled="outOfStock"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="1.5"
            stroke="currentColor"
            class="size-5"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              d="M12 4.5v15m7.5-7.5h-15"
            />
          </svg>
        </button>
      </div>

      <small
        x-show="!outOfStock && (quantity < minQuantity || quantity > maxQuantity)"
        x-cloak
        class="text-red-500"
      >
        Please enter a valid quantity (from <span x-text="minQuantity"></span> to <span x-text="maxQuantity"></span>).
      </small>

      <div x-show="availableVolumeDiscount" {{ 'x-cloak' if not product.volume_discounts }}>
        <div class="bg-white/[0.025] text-t-primary text-sm/50 text-center text-sm p-1.5 mt-2 w-full">
          Buy <span x-text="availableVolumeDiscount?.quantity - quantity"></span> more to get a <span x-text="availableVolumeDiscount?.percentage"></span>% discount.
        </div>
      </div>
    </div>
    <div class="mb-4">
      <label for="emailAddress" class="block mb-2 text-sm"
        >E-mail Address</label
      >
      <input
        type="email"
        class="bg-white/[0.025] text-t-primary text-sm py-2 px-4 w-full focus:border-accent-500 focus:ring-accent-500 focus:ring-2 focus:ring-opacity-50 focus:outline-none disabled:opacity-50"
        name="emailAddress"
        x-model="emailAddress"
        @input="emailInvalid = false"
        placeholder="name@domain.com"
        :disabled="outOfStock"
      />
      <small
        x-show="emailInvalid"
        x-cloak
        class="text-red-500"
      >
        Please enter a valid email address.
      </small>
    </div>
    {% if product.custom_fields|length > 0 %}
      <div class="custom-fields">
        {% for field in product.custom_fields %}
          <div class="mb-4">
            {% if field.type != 'checkbox' %}
              <div class="mb-2">
                <label for="{{ field.id }}" class="block text-sm">{{ field.name }} {{ '*' if field.is_required }}</label>
                {% if field.hint %}
                  <small class="text-t-primary/50">{{ field.hint }}</small>
                {% endif %}
              </div>
            {% endif %}
            {% if field.type == 'text' %}
              <input
                type="text"
                class="bg-white/[0.025] text-t-primary text-sm py-2 px-4 w-full focus:border-accent-500 focus:ring-accent-500 focus:ring-2 focus:ring-opacity-50 focus:outline-none disabled:opacity-50"
                name="{{ field.id }}"
                placeholder="{{ field.placeholder }}"
                x-model="customFields['{{ field.id }}']"
                :disabled="outOfStock"
              />
            {% endif %}
            {% if field.type == 'textarea' %}
              <textarea
                class="bg-white/[0.025] text-t-primary text-sm py-2 px-4 w-full focus:border-accent-500 focus:ring-accent-500 focus:ring-2 focus:ring-opacity-50 focus:outline-none disabled:opacity-50"
                name="{{ field.id }}"
                placeholder="{{ field.placeholder }}"
                x-model="customFields['{{ field.id }}']"
                :disabled="outOfStock"
              ></textarea>
            {% endif %}
            {% if field.type == 'checkbox' %}
              <label class="flex items-center gap-4 mb-2">
                <input
                  type="checkbox"
                  name="{{ field.id }}"
                  x-model="customFields['{{ field.id }}']"
                  :disabled="outOfStock"
                />
                <div>
                  <span class="block">{{ field.name }} {{ '*' if field.is_required }}</span>
                  {% if field.hint %}
                    <small class="text-t-primary/50">{{ field.hint }}</small>
                  {% endif %}
                </div>
              </label>
            {% endif %}
            {% if field.type == 'select' %}
              <select
                class="bg-white/[0.025] text-t-primary text-sm py-2.5 px-4 w-full focus:border-accent-500 focus:ring-accent-500 focus:ring-2 focus:ring-opacity-50 focus:outline-none disabled:opacity-50"
                name="{{ field.id }}"
                @change="() => customFields['{{ field.id }}'] = $event.target.value"
                :disabled="outOfStock"
              >
                {% for option in field.options %}
                  <option value="{{ option }}" :selected="customFields['{{ field.id }}'] == '{{ option }}'">
                    {{ option }}
                  </option>
                {% endfor %}
              </select>
            {% endif %}
            <small
              x-show="customFieldsInvalid?.['{{ field.id }}']"
              x-cloak
              class="text-red-500">
              Please enter {{ field.name }}.
            </small>
          </div>
        {% endfor %}
      </div>
    {% endif %}
    {% if coupons and coupons|length > 0 %}
    <div class="mb-4">
      <label for="couponCode" class="block mb-2 text-sm"
        >Coupon Code</label
      >
      <div class="flex overflow-hidden focus-within:border-accent-500 focus-within:ring-accent-500 focus-within:ring-2 focus-within:ring-opacity-50">
        <input
          type="input"
          class="w-[calc(100%-36px)] bg-white/[0.025] text-t-primary text-sm py-2 px-4 focus:outline-none disabled:opacity-50"
          name="couponCode"
          x-model="couponCode"
          @keydown.enter.prevent="checkCoupon"
          placeholder="MYCOUPON..."
          :disabled="outOfStock"
        />
        <button
          @click="checkCoupon"
          type="button"
          class="bg-white/5 hover:bg-accent-500 size-[36px] flex justify-center items-center transition-colors duration-300 ease-in-out disabled:pointer-events-none disabled:opacity-50"
          :disabled="outOfStock"
        >
          <template x-if="!isCheckingCoupon">
            <svg xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 256 256" class="size-5"><path d="M243.31,136,144,36.69A15.86,15.86,0,0,0,132.69,32H40a8,8,0,0,0-8,8v92.69A15.86,15.86,0,0,0,36.69,144L136,243.31a16,16,0,0,0,22.63,0l84.68-84.68a16,16,0,0,0,0-22.63ZM84,96A12,12,0,1,1,96,84,12,12,0,0,1,84,96Z"></path></svg>
          </template>
          <template x-if="isCheckingCoupon">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" class="size-5"><path fill="none" stroke="currentColor" stroke-dasharray="15" stroke-dashoffset="15" stroke-linecap="round" stroke-width="2" d="M12 3C16.9706 3 21 7.02944 21 12"><animate fill="freeze" attributeName="stroke-dashoffset" dur="0.3s" values="15;0"/><animateTransform attributeName="transform" dur="1.5s" repeatCount="indefinite" type="rotate" values="0 12 12;360 12 12"/></path></svg>
          </template>
        </button>
      </div>
      <small
        x-show="couponError"
        x-text="couponError"
        x-cloak
        class="text-red-500"></small>
      <small
        x-show="couponSuccess"
        x-text="couponSuccess"
        x-cloak
        class="text-green-500"></small>
    </div>
    {% endif %}
    {% if shop.discord_integration_enabled and product.discord_guild_id %}
    <div class="mb-4">
      <label class="block mb-2 text-sm">
        Discord {{ '*' if product.discord_required }}
      </label>
      <template x-if="discordUser !== null">
        <div class="inline-flex items-center gap-4 px-4 py-2 bg-white/[0.025] transition-colors duration-300 ease-in-out hover:bg-white/5">
          <img
            x-show="discordUser.avatar"
            :src="`https://cdn.discordapp.com/avatars/${discordUser.id}/${discordUser.avatar}.png`"
            :alt="discordUser.username"
            class="size-8 rounded-full"
          />
          <span x-show="!discordUser.avatar" class="size-8 flex items-center justify-center rounded-full bg-white/5 text-t-primary">
            <span x-text="discordUser.username.charAt(0)"></span>
          </span>
          <span class="text-t-primary" x-text="discordUser.username"></span>
          <button @click="disconnectDiscord" type="button" class="inline-flex items-center text-red-500" title="Disconnect Discord">
            <svg xmlns="http://www.w3.org/2000/svg" class="size-5" fill="currentColor" viewBox="0 0 256 256"><path d="M205.66,194.34a8,8,0,0,1-11.32,11.32L128,139.31,61.66,205.66a8,8,0,0,1-11.32-11.32L116.69,128,50.34,61.66A8,8,0,0,1,61.66,50.34L128,116.69l66.34-66.35a8,8,0,0,1,11.32,11.32L139.31,128Z"></path></svg>
          </button>
        </div>
      </template>
      <div x-show="discordUser === null">
        <button @click="connectDiscord" type="button" class="flex items-center gap-4 px-4 py-1.5 bg-white/[0.025] transition-colors duration-300 ease-in-out hover:bg-white/5 focus:border-accent-500 focus:ring-accent-500 focus:ring-2 focus:ring-opacity-50 focus:outline-none">
          <svg x-show="!connectDiscordInterval" xmlns="http://www.w3.org/2000/svg" class="size-5 text-[#5865f2]" viewBox="0 0 24 24"><path fill="currentColor" d="M19.27 5.33C17.94 4.71 16.5 4.26 15 4a.09.09 0 0 0-.07.03c-.18.33-.39.76-.53 1.09a16.09 16.09 0 0 0-4.8 0c-.14-.34-.35-.76-.54-1.09c-.01-.02-.04-.03-.07-.03c-1.5.26-2.93.71-4.27 1.33c-.01 0-.02.01-.03.02c-2.72 4.07-3.47 8.03-3.1 11.95c0 .02.01.04.03.05c1.8 1.32 3.53 2.12 5.24 2.65c.03.01.06 0 .07-.02c.4-.55.76-1.13 1.07-1.74c.02-.04 0-.08-.04-.09c-.57-.22-1.11-.48-1.64-.78c-.04-.02-.04-.08-.01-.11c.11-.08.22-.17.33-.25c.02-.02.05-.02.07-.01c3.44 1.57 7.15 1.57 10.55 0c.02-.01.05-.01.07.01c.11.09.22.17.33.26c.04.03.04.09-.01.11c-.52.31-1.07.56-1.64.78c-.04.01-.05.06-.04.09c.32.61.68 1.19 1.07 1.74c.03.01.06.02.09.01c1.72-.53 3.45-1.33 5.25-2.65c.02-.01.03-.03.03-.05c.44-4.53-.73-8.46-3.1-11.95c-.01-.01-.02-.02-.04-.02M8.52 14.91c-1.03 0-1.89-.95-1.89-2.12s.84-2.12 1.89-2.12c1.06 0 1.9.96 1.89 2.12c0 1.17-.84 2.12-1.89 2.12m6.97 0c-1.03 0-1.89-.95-1.89-2.12s.84-2.12 1.89-2.12c1.06 0 1.9.96 1.89 2.12c0 1.17-.83 2.12-1.89 2.12"/></svg>
          <svg x-show="connectDiscordInterval" xmlns="http://www.w3.org/2000/svg" class="size-5 text-[#5865f2]" viewBox="0 0 24 24" class="size-5"><path fill="none" stroke="currentColor" stroke-dasharray="15" stroke-dashoffset="15" stroke-linecap="round" stroke-width="2" d="M12 3C16.9706 3 21 7.02944 21 12"><animate fill="freeze" attributeName="stroke-dashoffset" dur="0.3s" values="15;0"/><animateTransform attributeName="transform" dur="1.5s" repeatCount="indefinite" type="rotate" values="0 12 12;360 12 12"/></path></svg>
          <span>Login with Discord</span>
          </span>
        </button>
        <small
          x-show="discordUserInvalid"
          x-cloak
          class="text-red-500"
        >
          Please connect your Discord account to be able to purchase this product.
        </small>
      </div>
    </div>
    {% endif %}
    <div class="mb-4">
      <label for="method" class="block mb-2 text-sm">Payment Method</label>
      <div class="flex flex-col gap-2" x-cloak>
        <template x-for="method in methods">
          <button
            type="button"
            :class="`flex items-center justify-between gap-4 min-h-[3.375rem] px-4 py-2 transition-colors duration-300 ease-in-out border disabled:opacity-50 ${paymentMethod == method.id ? 'bg-white/[0.025] border-accent-500' : 'bg-none border-white/5 hover:enabled:bg-white/[0.025]'}`"
            @click="() => {
              paymentMethod = method.id;
              methodInvalid = false;
            }"
            :disabled="outOfStock || (method.minAmount !== null && parseFloat(totalPrice) < method.minAmount) || (method.maxAmount !== null && parseFloat(totalPrice) > method.maxAmount)"
            >
              <div class="text-left truncate">
                <p x-text="method.name" class="text-sm font-medium truncate"></p>
                <p class="leading-none">
                  <template x-if="method.via">
                    <span class="inline-flex items-center gap-1 text-[0.7rem] text-white/50 truncate">
                      Powered by <span x-html="method.via" class="*:!mt-0 *:w-auto *:h-2.5"></span>
                    </span>
                  </template>
                  <template x-if="method.minAmount !== null && method.maxAmount !== null">
                    <span class="text-[0.7rem] text-t-primary/50 truncate">
                      <span x-show="method.via">•</span>
                      <span x-text="formatPrice(method.minAmount)"></span> - <span x-text="formatPrice(method.maxAmount)"></span>
                    </span>
                  </template>
                  <template x-if="method.minAmount !== null && method.maxAmount === null">
                    <span class="text-[0.7rem] text-t-primary/50 truncate">
                      <span x-show="method.via">•</span>
                      From <span x-text="formatPrice(method.minAmount)"></span>
                    </span>
                  </template>
                  <template x-if="method.minAmount === null && method.maxAmount !== null">
                    <span class="text-[0.7rem] text-t-primary/50 truncate">
                      <span x-show="method.via">•</span>
                      Up to <span x-text="formatPrice(method.maxAmount)"></span>
                    </span>
                  </template>
                  <template x-if="method.fee < 0">
                    <span class="text-[0.7rem] text-t-primary/50 truncate">
                      <span x-show="method.via || method.minAmount || method.maxAmount">•</span>
                      <span x-text="Math.abs(method.fee)"></span>% discount
                    </span>
                  </template>
                  <template x-if="method.fee > 0">
                    <span class="text-[0.7rem] text-t-primary/50 truncate">
                      <span x-show="method.via || method.minAmount || method.maxAmount">•</span>
                      <span x-text="method.fee"></span>% fee
                    </span>
                  </template>
                </p>
              </div>

              <div class="flex items-center gap-3">
                <template x-for="icon in method.icons">
                  <span x-html="icon" class="inline-flex items-center justify-center *:w-auto *:h-[1.125rem]"></span>
                </template>
              </div>
          </button>
        </template>
      </div>
      <small
        x-show="methodInvalid"
        x-cloak
        class="text-red-500"
      >
        Please select a payment method.
      </small>
    </div>

    {% if not is_embed %}
    <div class="mb-4">
      <label class="flex items-center gap-4 mb-2">
        <input type="checkbox" x-model="tos" :disabled="outOfStock" />
        <div>
          <span class="block text-xs">
            I have read and agree to {{ shop.name }}'s <a href="{{ '/terms' | shopUrl }}" target="_blank" class="text-accent-500">Terms of Service</a>.
          </span>
        </div>
      </label>
      <small x-show="tosInvalid" x-cloak class="text-red-500">
        Please accept the Terms of Service.
      </small>
    </div>
    {% endif %}

    <div class="mb-4">
      <label class="flex items-center gap-4 mb-2">
        <input type="checkbox" x-model="newsletter" value="1" :disabled="outOfStock" />
        <div>
          <span class="block text-xs">
            I would like to receive updates and promotions from {{ shop.name }}.
          </span>
        </div>
      </label>
    </div>

    <div class="mb-4 py-4 border-y border-y-white/5">
      <label class="block mb-2 text-sm">Your Order</label>
      <div>
        <div class="flex items-center justify-between mb-1">
          <span class="text-sm text-t-primary/50">
            Subtotal
          </span>
          <span class="text-sm text-t-primary/75">
            <span class="text-xs text-t-primary/50">
              <span x-text="quantity"></span>x <span x-text="formatPrice(singleUnitPrice)"></span> =
            </span>
            <span x-text="formatPrice(parseFloat(subTotalPrice).toFixed(2))"></span>
          </span>
        </div>
        <template x-if="appliedVolumeDiscount">
          <div class="flex items-center justify-between mb-1">
            <span class="text-sm text-t-primary/50">
              Volume Discount (<span x-text="appliedVolumeDiscount.percentage"></span>%)
            </span>
            <span class="text-sm text-t-primary/75">
              <span x-text="formatPrice(parseFloat(subTotalPrice * appliedVolumeDiscount.percentage / 100).toFixed(2))"></span>
            </span>
          </div>
        </template>
        <template x-if="appliedCoupon">
          <div class="flex items-center justify-between mb-1">
            <span class="text-sm text-t-primary/50">
              Coupon Code (<span x-text="appliedCoupon.code"></span>)
            </span>
            <span class="text-sm text-t-primary/75">
              <span x-text="formatPrice(parseFloat(appliedCouponDiscount).toFixed(2))"></span>
            </span>
          </div>
        </template>
        <template x-if="appliedPaymentMethodFee !== 0">
          <div class="flex items-center justify-between mb-1">
            <span class="text-sm text-t-primary/50">
              Payment Method
              <span x-text="appliedPaymentMethodFee > 0 ? 'Fee' : 'Discount'"></span>
              (<span x-text="appliedPaymentMethodFee"></span>%)
            </span>
            <span class="text-sm text-t-primary/75">
              <span x-text="formatPrice(parseFloat(subTotalPrice * appliedPaymentMethodFee / 100).toFixed(2))"></span>
            </span>
          </div>
        </template>
        <div class="flex items-center justify-between mt-1">
          <span class="text-sm text-t-primary font-bold">Total</span>
          <span class="text-sm text-t-primary font-bold">
            <span x-text="formatPrice(totalPrice)"></span>
          </span>
        </div>
      </div>
    </div>
    <template x-if="paymentError">
      <div class="bg-red-500 border border-white/5 text-t-primary p-4 my-4">
        <span x-text="paymentError"></span>
      </div>
    </template>
    <button
      type="submit"
      class="px-6 py-2 mt-4 w-full flex justify-center items-center gap-4 border font-semibold bg-accent-500 border-accent-500 text-t-accent hover:enabled:bg-accent-500/50 hover:enabled:text-t-primary transition-colors duration-300 ease-in-out disabled:opacity-50 disabled:pointer-events-none"
      :disabled="outOfStock || product.visibility == 'on_hold' || isSubmitting"
    >
      <template x-if="!isSubmitting">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 24 24"
          stroke-width="1.5"
          stroke="currentColor"
          class="size-5"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
